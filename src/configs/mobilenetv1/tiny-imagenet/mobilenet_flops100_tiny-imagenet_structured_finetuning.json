{
    "experiment_name": "ICIP22/structured-pruning",
    "data_dir": "data",
    "tensorboard": false,
    "dataset": "tiny-imagenet-200",
    "num_classes": 200,
    "input_channels": 3,
    "input_height": 224,
    "input_width": 224,
    "shift_pixels": 0,
    "augmentation": true,
    "mean": [0.485, 0.456, 0.406],
    "std": [0.229, 0.224, 0.225],

    "model": "Mobilev1CapsNet",
    "backbone_ratio_remain_flops": 100,
    "freeze": false,
    "start_epoch": 0,
    "epochs": 100,
    "batch_size": 128,
    "optimizer": "adam",
    "lr": 1e-5,
    "min_lr": 0,
    "scheduler": "exponential",
    "decay_rate": 0.99,
    "decay_steps": 1,
    "monitor": "loss",
    "iter_size": 1,
    "primary_num_routing_iterations": 0,
    "routing": "dynamic",
    "num_routing_iterations": 3,
    "patience": 10,
    "use_cuda": true,
    "cuda_device": "cuda:0",
    "num_workers": 4,
    "pin_memory": true,
    "print_every": 10,
    "save_every": 1,
    "seeds": [42],

    "input_channels_conv1": 1,
    "out_channels_conv1": 256,
    "kernel_size_conv1": 9,
    "stride_conv1": 1,
    "padding_conv1": 0,
    "dilation_conv1": 1,

    "dim_primaryCaps": [8,1],
    "squashing_primaryCaps": "hinton",
    "stride_primaryCaps": 1,
    "padding_primaryCaps": 0,
    "dilation_primaryCaps": 1,

    "dim_classCaps": [16,1],
    "transform_share_classCaps": false,

    "caps_loss": "margin",
    "batch_averaged": true,
    "margin_loss_lambda": 0.5,
    "m_minus": 0.1,
    "m_plus": 0.9,

    "cmap": "gray"
}